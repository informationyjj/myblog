<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.blogserver.mapper.IconedMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.blogserver.entity.Iconed">
        <id column="id" property="id" />
        <result column="bili_url" property="biliUrl" />
        <result column="qq_url" property="qqUrl" />
        <result column="git_url" property="gitUrl" />
        <result column="twitter_url" property="twitterUrl" />
        <result column="user_id" property="userId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, bili_url, qq_url, git_url, twitter_url, user_id
    </sql>

</mapper>
